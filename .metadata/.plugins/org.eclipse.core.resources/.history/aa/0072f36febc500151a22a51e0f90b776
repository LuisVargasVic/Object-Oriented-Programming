
public class Rectangle {
	private int length;
	private int width;
	private int x;
	private int y;
	public Rectangle(int i, int j, int k, int l) {
		// TODO Auto-generated constructor stub
	}
	public int area(){
		return length * width;
	}
	public static void main(String[] args){
		Rectangle stitch = new Rectangle (10,2,12,14);
		stitch.area();
	}
}
